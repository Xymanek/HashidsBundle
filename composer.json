{
    "name": "xymanek/hashids-bundle",
    "description": "Integrates hashids/hashids in a Symfony project with ability to define multiple domains",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Xymanek",
            "email": "xymanek@outlook.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Xymanek\\HashidsBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Xymanek\\HashidsBundle\\": "tests/"
        }
    },
    "require": {
        "php": "^7.0",

        "hashids/hashids": "^2.0",

        "symfony/dependency-injection": "^3.3 || ^4.0",
        "symfony/config": "^3.3 || ^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "matthiasnoback/symfony-config-test": "^3.1",
        "matthiasnoback/symfony-dependency-injection-test": "^2.3",

        "symfony/event-dispatcher": "^3.3 || ^4.0",
        "symfony/http-kernel": "^3.3 || ^4.0",
        "symfony/twig-bundle": "^3.3 || ^4.0",

        "sensio/framework-extra-bundle": "^5.1",
        "doctrine/annotations": "^1.6"
    },
    "minimum-stability": "stable"
}
